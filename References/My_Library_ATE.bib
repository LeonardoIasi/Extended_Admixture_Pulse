	pages = {457--469},
	file = {ScienceDirect Full Text PDF:/home/leonardo_iasi/Zotero/storage/AH4X6WIW/Lachance et al. - 2012 - Evolutionary History and Adaptation from High-Cove.pdf:application/pdf;ScienceDirect Snapshot:/home/leonardo_iasi/Zotero/storage/3F4GZRHX/S0092867412008318.html:text/html}
