	pages = {279--290},
	file = {PubMed Central Full Text PDF:/home/leonardo_iasi/Zotero/storage/SNYV5DSB/Hsieh et al. - 2016 - Whole-genome sequence analyses of Western Central .pdf:application/pdf}
